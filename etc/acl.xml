<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
        <resource id="Deki_Core::deki">
          <resource id="Deki_CustomerAddress::city" title="Customer Address" sortOrder="10">
            <resource id="Deki_CustomerAddress::city_save" title="Save city" sortOrder="10"/>
            <resource id="Deki_CustomerAddress::city_delete" title="Delete city" sortOrder="20"/>
            <resource id="Deki_CustomerAddress::city_update" title="Update city" sortOrder="30"/>
            <resource id="Deki_CustomerAddress::city_view" title="View city" sortOrder="40"/>
          </resource>
        </resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
              <resource id="Deki_Core::config_deki" title="Deki">
  							<resource id="Deki_CustomerAddress::config_deki_customeraddress" title="Customer Address"/>
              </resource>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
